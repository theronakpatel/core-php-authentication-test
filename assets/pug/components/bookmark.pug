// Bookmark Start
.col
    .bookmark.pull-right
        ul
            li
                a(href='#', data-bs-toggle='tooltip', title='Calender', data-bs-original-title='Calendar')
                    i(data-feather='calendar')
            li
                a(href='#', data-bs-toggle='tooltip', title='Mail', data-bs-original-title='Mail')
                    i(data-feather='mail')
            li
                a(href='#', data-bs-toggle='tooltip', title='Chat', data-bs-original-title='Chat')
                    i(data-feather='message-square')
            li
                a(href='javascript:void(0)')
                    i.bookmark-search(data-feather='star')
                form.form-inline.search-form
                    .form-group.form-control-search
                        input(type='text', placeholder='Search..')
// Bookmark Ends